{
  "name": "Nuxt 3 ESLint",
  "id": "nuxt3-eslint-mod",
  "inject": {
    "package.json": {
      "scripts": {
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix"
      },
      "devDependencies": {
        "eslint-plugin-vue": "latest",
        "eslint-plugin-jest": "latest",
        "prettier": "latest",
        "eslint": "latest",
        "@nuxtjs/eslint-config-typescript": "latest"
      }
    }
  },
  "compatibles": ["nuxt3-plate", "monorepo-plate"]
}
