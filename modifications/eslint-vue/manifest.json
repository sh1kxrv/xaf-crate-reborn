{
  "name": "Vue 3 ESLint",
  "id": "vue3-eslint-mod",
  "inject": {
    "package.json": {
      "scripts": {
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
      }
    }
  },
  "devInstall": [
    "eslint-plugin-vue",
    "eslint-plugin-jest",
    "prettier",
    "@rushstack/eslint-patch",
    "@vue/eslint-config-prettier"
  ],
  "compatibles": ["vue3-plate-lite", "vue3-plate"]
}
