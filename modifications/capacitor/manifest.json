{
  "name": "Capacitor",
  "id": "capacitor",
  "inject": {
    "package.json": {
      "devDependencies": {
        "@capacitor/cli": "latest"
      },
      "dependencies": {
        "@capacitor/core": "latest",
        "@capacitor/android": "latest",
        "@capacitor/keyboard": "latest",
        "@capacitor/splash-screen": "latest",
        "@capacitor/storage": "latest"
      },
      "scripts": {
        "cap:init": "npx cap init",
        "cap:sync": "npx cap sync",
        "cap:android:add": "npx cap add android",
        "cap:android:open": "npx cap open android",
        "cap:android:dev": "npx cap run android --livereload --external"
      }
    }
  },
  "compatibles": ["vue3-plate-ts", "vue3-electron-ts", "vue3-plate"]
}
