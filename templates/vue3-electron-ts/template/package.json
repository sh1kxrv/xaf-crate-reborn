    "@vue/eslint-config-prettier": "latest",
    "@vue/eslint-config-typescript": "latest",
    "esbuild": "^0.17.17",
    "eslint-plugin-vue": "latest",
    "prettier-plugin-organize-attributes": "latest",
    "sass": "latest",
    "sass-loader": "latest",
    "unplugin-auto-import": "latest",
    "unplugin-vue-macros": "latest",
    "vite": "latest",
    "vue-tsc": "^1.2.0"
