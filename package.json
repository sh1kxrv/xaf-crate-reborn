{
  "name": "xaf-crate-reborn",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test --parallel",
    "typecheck": "turbo run typecheck",
    "app:build": "turbo run build --scope=xfcr/cli",
    "app:start": "yarn app:build && yarn workspace @xfcr/cli run start"
  },
  "dependencies": {
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.17"
}
