{
  "name": "Vue 3 Prettier",
  "id": "vue3-prettier-mod",
  "inject": {
    "package.json": {
      "scripts": {
        "prettier:check": "prettier --check .",
        "prettier:write": "prettier --write ."
      },
      "devDependencies": {
        "prettier": "latest"
      }
    }
  },
  "compatibles": [
    "vue3-plate-lite",
    "vue3-plate",
    "nuxt3-plate",
    "monorepo-plate",
    "vue3-plate-ts"
  ]
}
