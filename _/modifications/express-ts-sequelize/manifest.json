{
  "name": "Express + Sequelize ORM | TS",
  "id": "express-ts-sequelize-mod",
  "inject": {
    "es-xaf.config.json": {
      "entrypoint": "src/app.ts"
    }
  },
  "devInstall": [
    "@types/express",
    "@types/node",
    "@types/validator",
    "@types/pg"
  ],
  "install": [
    "reflect-metadata",
    "pg",
    "pg-hstore",
    "sequelize",
    "sequelize-typescript",
    "express",
    "express-rate-limit",
    "kolorist"
  ],
  "compatibles": ["esbuild-node"],
  "conflicts": ["empty-bundler-js", "empty-bundler-ts"]
}
